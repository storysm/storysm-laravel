{
    "name": "storysm/storysm-laravel",
    "type": "project",
    "description": "The Storysm Laravel.",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "adhocore/jwt": "^1.1.3",
        "artesaos/seotools": "^1.3.2",
        "awcodes/filament-curator": "^3.7.3",
        "awcodes/filament-tiptap-editor": "^3.5.12",
        "awcodes/overlook": "^2.2.3",
        "bezhansalleh/filament-shield": "^3.3.5",
        "filament/filament": "^3.3.12",
        "intervention/image": "^2.7.2",
        "laravel-lang/common": "^6.7",
        "laravel/framework": "^12.9.2",
        "laravel/jetstream": "^5.3.6",
        "laravel/sanctum": "^4.0.8",
        "laravel/tinker": "^2.10.1",
        "lasserafn/php-initial-avatar-generator": "^4.4",
        "livewire/livewire": "^3.6.3",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2.4",
        "solution-forest/filament-translate-field": "^1.4.1",
        "spatie/color": "^1.8.0",
        "spatie/laravel-data": "^4.15.1",
        "spatie/laravel-google-fonts": "^1.4.3",
        "spatie/laravel-honeypot": "^4.5.4",
        "spatie/laravel-sitemap": "^7.3.6",
        "spatie/laravel-translatable": "^6.11.4",
        "verschuur/laravel-robotstxt": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.4",
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.3.1",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.22.0",
        "laravel/sail": "^1.41.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.0",
        "phpunit/phpunit": "^11.5.17",
        "prism-php/prism": "^0.61.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:host": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve --host 0.0.0.0\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev:host\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "npm run build",
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
